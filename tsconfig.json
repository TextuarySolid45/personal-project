{
  "extends": "astro/tsconfigs/strict",
  "BaseUrl": "./",
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "importsNotUsedAsValues": "error",
    "allowJs": true,
    "declaration": true,
    "outDir": "dist",
    "declarationMap": true,
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}